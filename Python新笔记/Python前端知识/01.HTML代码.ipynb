    "<!DOCTYPE html>\n",
    "<html>\n",
    "<head>\n",
    "<meta charset=\"utf-8\">\n",
    "<meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge,chrome=1\">\n",
    "<title>Examples</title>\n",
    "<meta name=\"description\" content=\"\">\n",
    "<meta name=\"keywords\" content=\"\">\n",
    "<link href=\"\" rel=\"stylesheet\">\n",
    "<style>\n",
    "\t* {\n",
    "\t\tmargin: 0;\n",
    "\t\tpadding: 0;\n",
    "\t}\n",
    "\n",
    "\t#header{\n",
    "\t\theight: 30px;\n",
    "\t\tbackground: rgba(0, 0, 0, 0.76);\n",
    "\t}\n",
    "\t.nav{\n",
    "\t\twidth: 800px;\n",
    "\t\theight: 30px;\n",
    "\t\tmargin: auto;\n",
    "\t\tcolor: white;\n",
    "\t}\n",
    "\tinput{\n",
    "\t\tbackground: white;\n",
    "\t}\n",
    "\n",
    "\t.left{\n",
    "\t\twidth: 500px;\n",
    "\t\theight: 800px;\n",
    "\t\tbackground: orange;\n",
    "\t\tfloat: left;\n",
    "\t}\n",
    "\n",
    "\t.right{\n",
    "\t\twidth: 200px;\n",
    "\t\theight: 800px;\n",
    "\t\tbackground: cyan;\n",
    "\t\tfloat: right;\n",
    "\t}\n",
    "\n",
    "\t.clear{\n",
    "\t\tclear: both;\n",
    "\t}\n",
    "\n",
    "\t.wrapping{\n",
    "\t\twidth: 800px;\n",
    "\t\theight: 800px;\n",
    "\t\tmargin: auto;\n",
    "\t\tborder-bottom: 5px solid red; \n",
    "\n",
    "\t}\n",
    "\t.slider{\n",
    "\t\twidth: 50px;\n",
    "\t\theight: 50px;\n",
    "\t\tbackground: blue;\n",
    "\t\tposition: fixed;\n",
    "\t\tcolor: white;\n",
    "\t\tbottom: 200px;\n",
    "\t\ttext-align: center;\n",
    "\t\tline-height: 50px;\n",
    "\t\tcursor: pointer;\n",
    "\t\tright: 50%;\n",
    "\t\tmargin-right: 500px;\n",
    "\t}\n",
    "\t#footer{\n",
    "\t\theight: 50px;\n",
    "\t\tbackground: rgba(0, 0, 0, 0.1);\n",
    "\t}\n",
    "\t.content{\n",
    "\t\theight: 50px;\n",
    "\t\twidth: 800px;\n",
    "\t\tfont-weight: bold;\n",
    "\t\tfont-style: italic;\n",
    "\t\tmargin: auto;\n",
    "\t}\n",
    "</style>\n",
    "</head>\n",
    "<body>\n",
    "    <div id=\"header\">\n",
    "    \t<div class=\"nav\">\n",
    "\t    \t这是导航\n",
    "\t    \t<input type=\"text\">\n",
    "\t    </div>\n",
    "    </div>\n",
    "\n",
    "\t<div class=\"body\">\n",
    "\t    <div class=\"wrapping\">\n",
    "\t\t\t<div class=\"left\"></div>\n",
    "\t\t\t<div class=\"right\"></div>\n",
    "\t\t\t<div class=\"clear\"></div>\n",
    "\t    </div>\n",
    "    </div>\n",
    "    <div class=\"slider\">△</div>\n",
    "\n",
    "    <div id=\"footer\">\n",
    "    \t<div class=\"content\">版权：</div>\n",
    "    \t\n",
    "    </div>\n",
    "</body>\n",
    "</html>"
   ]
