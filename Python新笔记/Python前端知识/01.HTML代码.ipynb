    "# 双列布局\n",
    "<!DOCTYPE html>\n",
    "<html>\n",
    "<head>\n",
    "<meta charset=\"utf-8\">\n",
    "<meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge,chrome=1\">\n",
    "<title>Examples</title>\n",
    "<meta name=\"description\" content=\"\">\n",
    "<meta name=\"keywords\" content=\"\">\n",
    "<link href=\"\" rel=\"stylesheet\">\n",
    "<style>\n",
    "\t.one{\n",
    "\t\twidth: 600px;\n",
    "\t\theight: 800px;\n",
    "\t\tbackground: orange;\n",
    "\t\tfloat: left;\n",
    "\t}\n",
    "\t.two{\n",
    "\t\twidth: 300px;\n",
    "\t\theight: 800px;\n",
    "\t\tbackground: cyan;\n",
    "\t\tfloat: left;\n",
    "\t}\n",
    "\t.parent{\n",
    "\t\twidth: 900px;\n",
    "\t\tmargin: auto; /*实现居中效果*/\n",
    "\t\tborder: 5px solid red;\n",
    "\t}\n",
    "\t.clear{\n",
    "\t\tclear: both; /*清除浮动，让后面的元素不活在前面元素的阴影之下*/\n",
    "\t}\n",
    "</style>\n",
    "</head>\n",
    "<body>\n",
    "\t<div class=\"parent\">\n",
    "\t\t<div class=\"one\"></div>\n",
    "\t\t<div class=\"two\"></div>\n",
    "\t\t<div class=\"clear\"></div>\n",
    "\t</div>\n",
    "</body>\n",
    "</html>"
