    "<!DOCTYPE html>\n",
    "<html>\n",
    "<head>\n",
    "<meta charset=\"utf-8\">\n",
    "<meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge,chrome=1\">\n",
    "<title>Examples</title>\n",
    "<meta name=\"description\" content=\"\">\n",
    "<meta name=\"keywords\" content=\"\">\n",
    "<link href=\"\" rel=\"stylesheet\">\n",
    "<style>\n",
    "\t* {\n",
    "\t\tmargin: 0;\n",
    "\t\tpadding: 0;\n",
    "\t}\n",
    "\tli {\n",
    "\t\tlist-style-type: none;\n",
    "\t}\n",
    "\ta {\n",
    "\t\ttext-decoration: none;\n",
    "\t}\n",
    "\timg{\n",
    "\t\theight: 800px;\n",
    "\t\twidth: 800px;\n",
    "\t}\n",
    "\t.showing{\n",
    "\t\theight: 800px;\n",
    "\t\twidth: 800px;\n",
    "\t\tposition: relative;\n",
    "\t\tmargin: auto;\n",
    "\t\tmargin-top: 50px;\n",
    "\t}\n",
    "\t.img_jihe{\n",
    "\t\twidth: 800px;\n",
    "\t\theight: 800px;\n",
    "\t\tmargin: auto;\n",
    "\t}\n",
    "\t.img_jihe img{\n",
    "\t\tposition: absolute;\n",
    "\t}\n",
    "\n",
    "\t.left_row, .right_row{\n",
    "\t\twidth: 100px;\n",
    "\t\theight: 100px;\n",
    "\t\tbackground: rgba(0, 0, 0, 0.5);\n",
    "\t\ttext-align: center;\n",
    "\t\tline-height: 100px;\n",
    "\t\tcolor: white;\n",
    "\t\tfont-size: 50px;\n",
    "\t\tposition: absolute;\n",
    "\t\ttop: 50%;\n",
    "\t\tz-index: 10;\n",
    "\t\tmargin-top: -50px;\n",
    "\n",
    "\t}\n",
    "\t.left_row{\n",
    "\t\tmargin-left: 40px;\n",
    "\t}\n",
    "\t.right_row{\n",
    "\t\tmargin-left: 720px;\n",
    "\t}\n",
    "\tli {\n",
    "\t\theight: 12px;\n",
    "\t\twidth: 12px;\n",
    "\t\tfloat: left;\n",
    "\t\tbackground: yellow;\n",
    "\t\tmargin: 5px;\n",
    "\t\ttext-align: center;\n",
    "\t\tcolor: black;\n",
    "\t\tline-height: 12px;\n",
    "\t\tborder-radius: 50%;\n",
    "\t}\n",
    "\tul {\n",
    "\t\tposition: absolute;\n",
    "\t\tbottom: 10px;\n",
    "\t\tleft: 50%;\n",
    "\t\tz-index: 11;\n",
    "\t\t\n",
    "\t}\n",
    "</style>\n",
    "</head>\n",
    "<body>\n",
    "    <div class=\"showing\">\n",
    "    \t<div class=\"img_jihe\">\n",
    "    \t\t<img id=\"img1\" src=\"1.jpg\" alt=\"\">1</img>\n",
    "    \t\t<img id=\"img2\" src=\"2.jpg\" alt=\"\">2</img>\n",
    "    \t\t<img id=\"img3\" src=\"3.jpg\" alt=\"\">3</img>\n",
    "    \t</div>\n",
    "\n",
    "\t\t<div class=\"left_row\">◁</div>\n",
    " \t\t<div class=\"right_row\">▷</div>\n",
    "\n",
    "    \t<ul>\n",
    "    \t\t<li></li>\n",
    "    \t\t<li></li>\n",
    "    \t\t<li></li>\n",
    "    \t\t<li></li>\n",
    "    \t\t<li></li>\n",
    "    \t</ul>\n",
    "    </div>\n",
    "</body>\n",
    "</html>"
   ]
