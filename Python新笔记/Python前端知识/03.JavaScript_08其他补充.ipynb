    "# 轮播图实例\n",
    "<!DOCTYPE html>\n",
    "<html>\n",
    "<head>\n",
    "<meta charset=\"utf-8\">\n",
    "<meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge,chrome=1\">\n",
    "<title>轮播图</title>\n",
    "<style>\n",
    "\t* {\n",
    "\t\tmargin: 0;\n",
    "\t\tpadding: 0;\n",
    "\t\toutline: 0;\n",
    "\t\tborder: 0;\n",
    "\t\tvertical-align: baseline;\n",
    "\t}\n",
    "\tli {\n",
    "\t\tlist-style: none;\n",
    "\t}\n",
    "\ta {\n",
    "\t\tcolor: #333;\n",
    "\t\ttext-decoration: none;\n",
    "\t}\n",
    "\t#wrap{\n",
    "\t\twidth: 400px;\n",
    "\t\theight: 582px;\n",
    "\t\tposition: relative;\n",
    "\t\tmargin: 100px auto 0;\n",
    "\t\t-webkit-user-select: none;\n",
    "\t\t-moz-user-select: none;\n",
    "\t\t-ms-user-select: none;\n",
    "\t\t-o-user-select: none;\n",
    "\t\tuser-select: none;\n",
    "\t}\n",
    "\t#imgs a {\n",
    "\t\tposition: absolute;\n",
    "\t\tdisplay: none;\n",
    "\t}\n",
    "\t#row span {\n",
    "\t\tdisplay: block;\n",
    "\t\twidth: 42px;\n",
    "\t\theight: 72px;\n",
    "\t\tfont-size: 36px;\n",
    "\t\tcolor: white;\n",
    "\t\tposition: absolute;\n",
    "\t\ttop: 50%;\n",
    "\t\tmargin-top: -36px;\n",
    "\t\ttext-align: center;\n",
    "\t\tline-height: 72px;\n",
    "\t\tfont-weight: bold;\n",
    "\t\tbackground: rgb(33, 33, 33);\n",
    "\t\tcursor: pointer; /*添加小手指*/\n",
    "\t\tfont-family: : serif;\n",
    "\t}\n",
    "\t#row span:first-child{\n",
    "\t\tleft: 0;\n",
    "\t}\n",
    "\t#row span:last-child{\n",
    "\t\tright: 0;\n",
    "\t}\n",
    "\t#btn{\n",
    "\t\toverflow: hidden; /*清除浮动*/\n",
    "\t\tposition: absolute;\n",
    "\t\tbottom: 16px;\n",
    "\t\tleft: 50%;\n",
    "\t\tmargin-left: -45px;\n",
    "\n",
    "\t}\n",
    "\t#btn li{\n",
    "\t\tfloat: left;\n",
    "\t\twidth: 20px;\n",
    "\t\theight: 20px;\n",
    "\t\tbackground: #fff;\n",
    "\t\tcolor: cyan;\n",
    "\t\tcursor: pointer;\n",
    "\t\tborder-radius: 50%;\n",
    "\t\tmargin: 0 5px;\n",
    "\t}\n",
    "\t#btn li.on{\n",
    "\t\tbackground-color: blue;\n",
    "\t}\n",
    "\t\n",
    "</style>\n",
    "</head>\n",
    "<body>\n",
    "    <div id=\"wrap\">\n",
    "    \t<div id=\"imgs\">\n",
    "    \t\t<a href=\"javascript: void(0);\"><img width=\"400px\" height=\"582\" src=\"1.jpg\" alt=\"\"></a>\n",
    "    \t\t<a href=\"javascript: void(0);\"><img width=\"400px\" height=\"582\" src=\"2.jpg\" alt=\"\"></a>\n",
    "    \t\t<a href=\"javascript: void(0);\"><img width=\"400px\" height=\"582\" src=\"3.jpg\" alt=\"\"></a>\n",
    "    \t</div>\n",
    "    \t<div id=\"row\">\n",
    "    \t\t<span class=\"previous\">&lt;</span>\n",
    "    \t\t<span class=\"next\">&gt;</span>\n",
    "    \t</div>\n",
    "    \t<ul id=\"btn\">\n",
    "    \t\t<li></li>\n",
    "    \t\t<li></li>\n",
    "    \t\t<li></li>\n",
    "    \t</ul>\n",
    "    </div>\n",
    "</body>\n",
    "</html>\n",
    "<script>\n",
    "\tvar wrap = document.getElementById(\"wrap\");\n",
    "\tvar imgs = document.getElementById(\"imgs\").getElementsByTagName(\"a\");\n",
    "\tvar row_spans = document.getElementById(\"row\").getElementsByTagName(\"span\");\n",
    "\tvar btn = document.getElementById(\"btn\").getElementsByTagName(\"li\");\n",
    "\tvar timer = 0;//计时器\n",
    "\tvar index = 0;//图片索引\n",
    "\n",
    "\timgs[0].style.display = \"block\";\n",
    "\tbtn[0].className = \"on\";\n",
    "\n",
    "\t//图片的点击\n",
    "\tfor (var i=0; i<btn.length; i++){\n",
    "\t\tbtn[i].index = i;\n",
    "\t\tbtn[i].onclick = function(){\n",
    "\t\t\tbtn[index].className = \"\";\n",
    "\t\t\timgs[index].style.display = \"none\";\n",
    "\t\t\tindex = this.index;\n",
    "\t\t\tbtn[index].className = \"on\";\n",
    "\t\t\timgs[index].style.display = \"block\";\n",
    "\t\t};\n",
    "\t\t//禁止拖拽图片\n",
    "\t\timgs[i].children[0].onmousedown = function(e){\n",
    "\t\t\te = e || event;\n",
    "\t\t\te.preventDefault ? e.preventDefault(): window.event.returnValue = false;\n",
    "\t\t}\n",
    "\t}\n",
    "\n",
    "\tfor (var i=0; i<row_spans.length; i++){\n",
    "\t\t//箭头的点击，点击下一张\n",
    "\t\tif (i){\n",
    "\t\t\trow_spans[i].onclick = function(){\n",
    "\t\t\t\tbtn[index].className = \"\";\n",
    "\t\t\t\timgs[index].style.display = \"none\";\n",
    "\t\t\t\tindex++;\n",
    "\t\t\t\tindex = index % imgs.length;\n",
    "\t\t\t\tbtn[index].className = \"on\";\n",
    "\t\t\t\timgs[index].style.display = \"block\";\n",
    "\t\t\t}\n",
    "\t\t}else{\n",
    "\t\t\trow_spans[i].onclick = function(){\n",
    "\t\t\t\tbtn[index].className = \"\";\n",
    "\t\t\t\timgs[index].style.display = \"none\";\n",
    "\t\t\t\tindex--;\n",
    "\t\t\t\tif (index<0) index = imgs.length-1;\n",
    "\t\t\t\tbtn[index].className = \"on\";\n",
    "\t\t\t\timgs[index].style.display = \"block\";\n",
    "\t\t\t}\n",
    "\t\t}\n",
    "\t}\n",
    "\t\n",
    "\t//自动滚动播放\n",
    "\tfunction auto(){\n",
    "\t\ttimer = setInterval(function(){\n",
    "\t\t\tbtn[index].className = \"\";\n",
    "\t\t\timgs[index].style.display = \"none\";\n",
    "\t\t\tindex++;\n",
    "\t\t\tindex = index % imgs.length;\n",
    "\t\t\tbtn[index].className = \"on\";\n",
    "\t\t\timgs[index].style.display = \"block\";\n",
    "\t\t}, 500);\n",
    "\t}\n",
    "\tauto();\n",
    "\twrap.onmouseover = function(){\n",
    "\t\tclearInterval(timer);\n",
    "\t}\n",
    "\twrap.onmouseout = function(){\n",
    "\t\tauto();\n",
    "\t}\n",
    "\n",
    "\n",
    "\n",
    "</script>"
   ]
  }
